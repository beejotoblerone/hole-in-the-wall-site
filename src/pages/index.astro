---

import Layout from "@layouts/Layout.astro";
import SampleComponent from "@components/flatbreadsection.astro";
import Container from "@components/Container.astro";
import TwoColumns from "@components/TwoColumns.astro";
import AstroLogo from "@assets/images/logo.png";
import { Image } from "astro:assets";
import PropCard from "@components/PropCard.astro";


---

<Layout title="Astro Starter">
  <Container>
    <Image src={AstroLogo} alt="" />
    <h1>a simple Astro Starter Kit</h1>
    <TwoColumns>
      <h2 slot="left">This is the left column</h2>
      <p slot="right">
        This is the right column. You can put anything you want in either column.
      </p>
    </TwoColumns>
    <SampleComponent text="This is a sample component!" />

  </Container>
</Layout>
